y.slice(0, this.state.stepNumber + 1);\n        const current = history[history.length - 1];\n        const squares = current.squares.slice();\n        if (calculateWinner(squares) || squares[i]) {\n            return;\n        }\n        squares[i] = this.state.xIsNext ? \"X\" : \"O\";\n        this.setState({\n            history: history.concat([\n                {\n                    squares: squares\n                }\n            ]),\n            stepNumber: history.length,\n            xIsNext: !this.state.xIsNext\n        });\n    }\n\n    jumpTo(step) {\n        this.setState({\n            stepNumber: step,\n            xIsNext: (step % 2) === 0\n        });\n    }\n\n    render() {\n        const history = this.state.history;\n        const current = history[this.state.stepNumber];\n        const winner = calculateWinner(current.squares);\n\n        const moves = history.map((step, move) => {\n            const desc = move ?\n                'Go to move #' + move :\n                'Go to game start';\n            return (\n                <li key={move}>\n                    <button onClick={() => this.jumpTo(move)}>{desc}</button>\n                </li>\n            );\n        });\n\n        let status;\n        if (winner) {\n            status = \"Winner: \" + winner;\n        } else {\n            status = \"Next player: \" + (this.state.xIsNext ? \"X\" : \"O\");\n        }\n\n        return (\n            <div className=\"game\">\n                <div className=\"game-board\">\n                    <Board\n                        squares={current.squares}\n                        onClick={i => this.handleClick(i)}\n                    />\n                </div>\n                <div className=\"game-info\">\n                    <div>{status}</div>\n                    <ol>{moves}</ol>\n                </div>\n            </div>\n        );\n    }\n}\n\n// ========================================\n\nfunction calculateWinner(squares) {\n    const lines = [\n        [0, 1, 2],\n        [3, 4, 5],\n        [6, 7, 8],\n        [0, 3, 6],\n        [1, 4, 7],\n        [2, 5, 8],\n        [0, 4, 8],\n        [2, 4, 6]\n    ];\n    for (let i = 0; i < lines.length; i++) {\n        const [a, b, c] = lines[i];\n        if (squares[a] && squares[a] === squares[b] && squares[a] === squares[c]) {\n            return squares[a];\n        }\n    }\n    return null;\n}\n",["39","40"],"/Users/wangshuaishuai/cs-github/github/my-app/src/2.react_tab/tab.js",[],"/Users/wangshuaishuai/cs-github/github/my-app/src/2.react_tab/react_tab.js",[],"/Users/wangshuaishuai/cs-github/github/my-app/src/0.MAIN CONCEPTS/4.Components and Props/5.1.js",[],"/Users/wangshuaishuai/cs-github/github/my-app/src/0.MAIN CONCEPTS/5. State and Lifecycle/6.2.js",[],"/Users/wangshuaishuai/cs-github/github/my-app/src/0.MAIN CONCEPTS/7. Conditional Rendering/2.2.js",[],"/Users/wangshuaishuai/cs-github/github/my-app/src/0.MAIN CONCEPTS/7. Conditional Rendering/5.2.js",["41"],{"ruleId":"42","severity":1,"message":"43","line":2,"column":9,"nodeType":"44","messageId":"45","endLine":2,"endColumn":12},{"ruleId":"42","severity":1,"message":"46","line":3,"column":9,"nodeType":"44","messageId":"45","endLine":3,"endColumn":21},{"ruleId":"42","severity":1,"message":"47","line":2,"column":8,"nodeType":"44","messageId":"45","endLine":2,"endColumn":16},{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"42","severity":1,"message":"52","line":1,"column":19,"nodeType":"44","messageId":"45","endLine":1,"endColumn":28},"no-unused-vars","'App' is defined but never used.","Identifier","unusedVar","'LoginControl' is defined but never used.","'ReactDOM' is defined but never used.","no-native-reassign",["53"],"no-negated-in-lhs",["54"],"'useEffect' is defined but never used.","no-global-assign","no-unsafe-negation"]