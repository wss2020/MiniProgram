/**app.wxss**/
@import "wxParse/wxParse.wxss";


.head{
   position:fixed;
   top:0px;
   left:0px;
   z-index: 100;
   width:100%;
   height:73px;
   line-height:73px;
   font-size: 32rpx;
   background: #f2f2f2;
 }

/*  加载更多   */
.clear{
    clear:both;
    height:0;
    overflow:hidden;
}



/* 去除button默认样式 */
button::after {
    border: none;
}
button {
    background-color: transparent;
    padding-left: 0;
    padding-right: 0;
    line-height:inherit;
}
button {
    border-radius:0;
}
input{
    outline:none;
    border:none;
    list-style: none;
}


/*页面底部提示*/
.bottom_taps {margin: 9px;font-size: 30rpx;text-align: center;color: #999;}


.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}


