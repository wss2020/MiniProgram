/* pages/giftbag/record/record.wxss */

Page{
    background: white;
}


.add_bag {
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    width: 100%;
    min-height: 100rpx;
    background: #eeeeee;
    z-index: 11;
}

.add_bag  image{
    margin: 20rpx 0 10rpx;
    width: 710rpx;
    height: 80rpx;
}

.submit {
    display: flex;
    justify-content: center;
    align-items: center;
    width: 600rpx;
    height: 80rpx;
    color: #fff;
    background: #e33d3d;
    border-radius: 200rpx;
    margin: 10rpx 10rpx;
}
